<script type="text/javascript">
  var chart;
  $(document).ready(function() {
    var chart1; // globally available
    $(document).ready(function() {
      chart1 = new Highcharts.Chart({
        chart: {
          renderTo: 'container',
          type: 'bar'
        },
        title: {
          text: 'Stock Ranges'
        },
        xAxis: {
          categories: ['Above Rs 900','800-900','700-800','600-700', '500-600','400-500','300-400','200-300', '100-200', '0-100']
        },
        yAxis: {
          title: {
            text: 'Number of Stocks'
          }
        },
        series: [{
            name: 'Count',
            data: [<%= @range_and_average[:range] %>]
          },{
            name: 'Average Price',
            data: [<%= @range_and_average[:average] %>]
          }
          ]
      });
    });
  });

</script>
</head>

<body>

  <div id="container" style="width: 800px; height: 400px; margin: 0 auto"></div>

</body>
</html>
